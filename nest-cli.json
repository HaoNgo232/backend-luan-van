{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/gateway",
  "projects": {
    "ar-app": {
      "type": "application",
      "root": "apps/ar-app",
      "entryFile": "main",
      "sourceRoot": "apps/ar-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ar-app/tsconfig.app.json"
      }
    },
    "cart-app": {
      "type": "application",
      "root": "apps/cart-app",
      "entryFile": "main",
      "sourceRoot": "apps/cart-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cart-app/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "order-app": {
      "type": "application",
      "root": "apps/order-app",
      "entryFile": "main",
      "sourceRoot": "apps/order-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order-app/tsconfig.app.json"
      }
    },
    "payment-app": {
      "type": "application",
      "root": "apps/payment-app",
      "entryFile": "main",
      "sourceRoot": "apps/payment-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/payment-app/tsconfig.app.json"
      }
    },
    "product-app": {
      "type": "application",
      "root": "apps/product-app",
      "entryFile": "main",
      "sourceRoot": "apps/product-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/product-app/tsconfig.app.json"
      }
    },
    "report-app": {
      "type": "application",
      "root": "apps/report-app",
      "entryFile": "main",
      "sourceRoot": "apps/report-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/report-app/tsconfig.app.json"
      }
    },
    "user-app": {
      "type": "application",
      "root": "apps/user-app",
      "entryFile": "main",
      "sourceRoot": "apps/user-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-app/tsconfig.app.json"
      }
    }
  }
}