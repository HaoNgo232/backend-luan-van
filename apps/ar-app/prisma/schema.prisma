datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_AR")
}

generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

model ARSnapshot {
  id        String   @id @default(cuid())
  userId    String?
  productId String
  imageUrl  String
  metadata  Json?
  createdAt DateTime @default(now())
}