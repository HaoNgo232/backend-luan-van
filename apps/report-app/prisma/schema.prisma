datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_REPORT")
}

generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

model ReportEntry {
  id        String   @id @default(cuid())
  type      String
  payload   Json?
  fromAt    DateTime?
  toAt      DateTime?
  createdAt DateTime @default(now())
}